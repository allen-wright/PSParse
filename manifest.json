{
  "name": "PSParse",
  "description": "Parses the late 2020 PlayStation Store redesign.",
  "version": "0.3",
  "icons": {
    "16": "images/psp16.png",
    "32": "images/psp32.png",
    "192": "images/psp192.png",
    "512": "images/psp512.png"
  },
  "permissions": [
    "storage", "declarativeContent", "activeTab"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_title": "Parse the PS Store.",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/psp16.png",
      "32": "images/psp32.png",
      "192": "images/psp192.png",
      "512": "images/psp512.png"
    }
  },
  "manifest_version": 2
}